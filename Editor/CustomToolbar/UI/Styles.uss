.toolbar-left-zone {
    display: flex;
    flex-grow: 1;
    justify-content: flex-end;
    align-items: flex-end;
    flex-direction: row;
}

.toolbar-right-zone {
    display: flex;
    flex-grow: 1;
    justify-content: flex-start;
    align-items: flex-end;
    flex-direction: row;
}

.toolbar-btn-invoke-menu {
    height: 20px;
    width: 26px;
    padding: 0 0 0 0;
    font-size: 15px;
    flex-shrink: 0;
    -unity-text-align: middle-center;
}

.setting-horizontal-panel {
    flex-direction: row;
}

.setting-listview {
    flex-direction: column;
}

.setting-toggle {
    width: 20px;
    height: 20px;
    margin-bottom: 0;
    margin-top: 0;
}

.setting-label-display {
    width: 200px;
    font-size: 13;
    -unity-font-style: bold;
}

.setting-text-invoke-menu-display {
    width: 300px;
}

.setting-text-invoke-menu-path {
    width: 600px;
    margin-left: 50px;
}
